{{#.}}
<li class="list-group-title">{{letter}}</li>
	{{#each list}}
		<li data-id="{{id}}" class="church-item">
			<a href="/static/church.html?id={{id}}" class="item-link">
				<div class="item-content">
					<div class="item-media"><img src="/static/img/church.png" width="44"></div>
					<div class="item-inner">
						<div class="item-title-row">
							<div class="item-title">{{name}}</div>
						</div>
						<div class="item-subtitle">{{address}}</div>
					</div>
				</div>
			</a>
		</li>
	{{/each}}
{{/.}}